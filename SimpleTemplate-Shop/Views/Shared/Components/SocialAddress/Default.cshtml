@model IEnumerable<AppSocialAddress>

@if (Model.Count() == 0)
{
    <p class="text-danger">Please add a social link/s</p>
}
else
{
    @foreach (var item in Model)
    {
        <a href="@item.UrlAddress" target="_blank" class="btn d-inline-block">
            <img style="border-radius:50%;" src="~/images/@item.AppSocialImg" height="50" width="50" asp-append-version="true" />
        </a>
    }
}