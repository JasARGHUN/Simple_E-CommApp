@model Info

@{
    var photoPath = "~/images/" + (Model.AppImage ?? "shop-icon-png.jpg");
}

<a asp-controller="Home" asp-action="Index">
    <img class="mr-5" style="float:left; border-radius: 50px;" src="@photoPath" height="40" width="40" asp-append-version="true" alt="" />
</a>
<h5 class="mt-2 mr-5 text-dark" style="text-shadow: 1px 1px 1px #aba8a8; font-size:2em;">@await Component.InvokeAsync("AppName")</h5>